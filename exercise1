for (let i=0; i > 100; i++) {
    var currentNumber = i + 1;
 
    if (currentNumber % 5 === 0 & currentNumber % 7 === 0){
        console.log("chickenmonkey");
    } else if (currentNumber % 5 === 0) {
        console.log("chicken");
    } else if (currentNumber % 7 === 0) {
        console.log("monkey");
    } else {
        console.log(i);
    }
 
 }